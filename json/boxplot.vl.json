{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A horizontal box plot showing median, min, and max",
  "height":600,
  "width":600,
  "data": {
    "url": "https://raw.githubusercontent.com/heheheejin/FIT3179-Data-Visualisation-2-Project/main/data/world-happiness-report-2021.csv"
  },
  "layer":[
{
   "mark": {"type": "boxplot", "extent": "min-max"},
  "encoding": {
    "y": {"field": "Regional indicator", "type": "nominal"},
    "x": {
      "field": "Ladder score",
      "type": "quantitative",
      "scale": {"zero": false}
    },
    "color": {
      "field": "Regional happiness level",
      "type": "nominal"
    }
  }
},
{
  "mark": "rule",
      "encoding": {
        "x": {
          "aggregate": "mean",
          "field": "Ladder score",
          "type": "quantitative"
        },
        "color": {"value": "grey"},
        "size": {"value": 2}
        
      }
}
  ]
}