{
      "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
      "data": {"url": "https://raw.githubusercontent.com/heheheejin/FIT3179-Data-Visualisation-2/main/data/world-happiness-report.csv"},  
      "transform": [
        {"filter": {"field": "Country", "oneOf": [
          "Denmark","Australia","United Kingdom","India","South Africa","Chile","Jordan","Cambodia","Bulgaria","United States of America","Egypt","Greece","Serbia","Latvia"]
        }
        },
        {"filter": {
          "field": "year", "oneOf" : [2007,2020]
        }}
      ],
      "mark": "line",
      "width": {"step": 200},
      "encoding": {
        "x": {
          "field": "year",
          "type": "ordinal",
          "scale": {"padding": 0.5,"zero":false}
        },
        "y": {
          "field": "Life Ladder",
          "type": "quantitative",
          "scale": {"padding": 0.5,"zero":false}
    
        },
        "color": {"field": "Country", "type": "nominal"},
        "tooltip":
          {"field":"Country", "type": "nominal"}
      },
      "height":600
    }
    